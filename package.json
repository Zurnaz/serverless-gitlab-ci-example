{
  "name": "serverless-gitlab-ci",
  "version": "1.0.0",
  "description": "Repo for deploying serverless gitlab ci",
  "main": "index.js",
  "author": "bogdandrema@gmail.com",
  "license": "MIT",
  "scripts": {
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js .",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,md}\"",
    "lint": "run-s lint:all:*",
    "unit": "jest --config=jest.config.js",
    "ci": "jest --ci --coverage",
    "e2e": "cypress run",
    "watch": "jest --watch",
    "verify": "cypress verify",
    "build": "LOCAL_PLUGIN=true NODE_ENV='development' SLS_DEBUG=* sls package",
    "deploy": "LOCAL_PLUGIN=true SLS_DEBUG=* sls deploy -s dev"
  },
  "dependencies": {
    "middy": "0.29.0"
  },
  "devDependencies": {
    "@anttiviljami/serverless-stack-output": "0.3.1",
    "aws-sdk": "2.527.0",
    "aws-sdk-mock": "4.5.0",
    "chai": "4.2.0",
    "cypress": "3.4.1",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-ava": "8.0.0",
    "eslint-plugin-cypress": "2.6.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "4.0.1",
    "jest": "24.9.0",
    "markdownlint-cli": "0.18.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "serverless-domain-manager": "3.3.0",
    "serverless-webpack": "5.3.1",
    "webpack": "4.39.3"
  }
}
